   "source": [
    "num = int(input(\"Please enter a number: \"))\n",
    "result_list = []\n",
    "for i in range(2,num):\n",
    "    prime = 0\n",
    "    for x in range(2,num):\n",
    "        if i % x == 0:\n",
    "            prime += 1\n",
    "    if prime == 1:\n",
    "        result_list.append(i)\n",
    "print(result_list)"
   ]
  }
